	author = {Friese, Heidrun and Nolden, Marcus and Rebane, Gala and Schreiter, Miriam},
	month = mar,
	year = {2020},
	note = {Google-Books-ID: rRDUDwAAQBAJ},
	keywords = {Social Science / Media Studies, Social Science / Sociology / General, Social Science / Sociology of Religion},
}

